{
  "version": 2,
  "functions": {
    "api/spotify-callback.js": {
      "maxDuration": 30
    },
    "api/youtube-callback.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/hijri-date.js": {
      "maxDuration": 10
    },
    "api/next-prayer.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/callback",
      "dest": "/api/spotify-callback.js"
    },
    {
      "src": "/youtube-callback",
      "dest": "/api/youtube-callback.js"
    },
    {
      "src": "/health/youtube",
      "dest": "/api/youtube-callback.js"
    },
    {
      "src": "/health/spotify",
      "dest": "/api/spotify-callback.js"
    },
    {
      "src": "/api/hijri-date",
      "dest": "/api/hijri-date.js"
    },
    {
      "src": "/api/next-prayer",
      "dest": "/api/next-prayer.js"
    }
  ]
}
